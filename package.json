{
  "name": "arduino-website-integration",
  "version": "1.0.0",
  "description": "Real-time communication between an Arduino and a web interface using Node.js and WebSockets",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [
